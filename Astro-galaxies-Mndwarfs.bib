month = {may},
number = {2},
pages = {L83--L86},
primaryClass = {astro-ph},
title = {{On Variations in the Peak Luminosity of Type Ia Supernovae}},
url = {http://stacks.iop.org/1538-4357/590/i=2/a=L83 http://arxiv.org/abs/astro-ph/0305114 http://dx.doi.org/10.1086/376721},
