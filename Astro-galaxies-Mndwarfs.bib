arxivId = {1404.7144},
author = {Weisz, Daniel R. and Dolphin, Andrew E. and Skillman, Evan D. and Holtzman, Jon and Gilbert, Karoline M. and Dalcanton, Julianne J. and Williams, Benjamin F.},
