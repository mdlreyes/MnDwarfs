file = {:Users/miadelosreyes/Library/Application Support/Mendeley Desktop/Downloaded/Allen, Porto De Mello - 2010 - Mn, Cu, and Zn abundances in barium stars and their correlations with neutron capture element.pdf:pdf},
issn = {0004-6361},
journal = {A{\&}A},
keywords = {abundances-stars,chemically peculiar-stars,late-type-techniques,spectroscopic,stars},
month = {jan},
pages = {A63},
title = {{Mn, Cu, and Zn abundances in barium stars and their correlations with neutron capture elements}},
