issn = {0004-6256},
journal = {Astron. Journal, Vol. 124, Issue 6, pp. 3222-3240.},
keywords = {Cosmology: Dark Matter,Galaxies: Dwarf,Galaxies: Fundamental Parameters,Galaxies: Individual: Name: Draco,Galaxies: Individual: Name: Ursa Minor,Galaxies: Local Group,Galaxies: Stellar Content,Galaxies: Structure},
month = {sep},
