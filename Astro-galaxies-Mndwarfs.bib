doi = {10.1051/0004-6361/201118636},
file = {:Users/miadelosreyes/Library/Application Support/Mendeley Desktop/Downloaded/North et al. - 2012 - Astrophysics Manganese in dwarf spheroidal galaxies.pdf:pdf},
journal = {A{\&}A},
keywords = {HRS,HRS:Carina,HRS:Fornax,HRS:Sculptor,HRS:Sextans,abundances-galaxies,dwarf-galaxies,evolution-galaxies,formation,stars,stellar content-galaxies},
