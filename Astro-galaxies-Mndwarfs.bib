eprint = {0805.0554},
issn = {0004-6361},
journal = {A{\&}A},
month = {aug},
number = {3},
pages = {951--970},
title = {{A grid of MARCS model atmospheres for late-type stars}},
url = {http://arxiv.org/abs/0805.0554 http://dx.doi.org/10.1051/0004-6361:200809724 http://www.aanda.org/10.1051/0004-6361:200809724},
volume = {486},
